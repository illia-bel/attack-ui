<ifModule mod_rewrite.c>
    RewriteEngine On
    RewriteBase /
    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteRule (.*) /index.html [QSA,L]
</ifModule>
RewriteCond %{REQUEST_URI} !^/promo/

<FilesMatch "\.(json)$">
  Header unset Cache-Control
</FilesMatch>